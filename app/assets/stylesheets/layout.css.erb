.logged-in-navbar .col-not-8 {
  background-color: white;
  padding: 1%;
  width: 9%;
  opacity: .8;
  border-radius: 3px;
}
.logged-in-navbar .col-not-8 a {
  color: #045638;
  opacity: 1;
  text-decoration: none;
}

.logged-in-navbar .col-8 {
  background-color: white;
  padding: 1%;
  width: 7%;
  opacity: .8;
  border-radius: 3px;
}

.logged-in-navbar .col-8 a {
  color: #045638;
  opacity: 1;
  text-decoration: none;
}

.logged-in-navbar .col-all-8 {
}



.logged-in-navbar .title {
  text-align: center;
  opacity: .95;
  font-size: 40px;
  color: white;
  text-decoration: underline overline;
  margin-bottom: 2%;
}


.logged-in-navbar .title a:visited {
    color: white;
}

.logged-in-navbar {
  background-color: #5D7843;
  padding: 1%;
  opacity: .8;
  text-align: center;
  overflow: auto;
  border-radius: 3px;
}

.logged-out-title-bar {
  background-color: #5D7843;
  padding: 1%;
  opacity: .8;
  text-align: center;
  overflow: auto;
  border-radius: 3px;
}

.logged-out-title-bar .title {
  text-align: center;
  opacity: .95;
  font-size: 40px;
  color: white;
  text-decoration: underline overline;
  margin-top: 1%;
  margin-bottom: 1%;
}

.logged-out-title-bar .title a:visited {
  color: white;
}

.footer {
  background-color: #5D7843;
  padding: 2%;
  opacity: .8;
  overflow: auto;
  border-radius: 3px;
  content: "";
  margin-top: 10%;
  clear: left;
  
}

.footer .greeting {
  color: #1E4800;
  font-size: 22px;
  float: left;
  line-height: 30px;
  background-color: white;
  padding: 1%;
  opacity: .8;
  border-radius: 3px;
}

.footer #log_out {
  font-size: 22px;
  text-align: left;
  background-color: white;
  padding: 1%;
  width: 7%;
  opacity: .8;
  border-radius: 3px;
  float: right;
  line-height: 30px;
}

.footer #log_out a {
  color: #045638;
  opacity: 1;
  text-decoration: none;
}

